{
    "name": "flyerless/flyerless-club-management",
    "description": "A module to allow club captains and other authorised committee members to modify their clubs details on the Flyerless system",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Warren",
            "email": "tristan@flyerless.co.uk"
        }
    ],
    "require": {
        "php": "^7.2",
        "bristol-su/support": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Flyerless\\FlyerlessClubManagement\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flyerless\\FlyerlessClubManagement\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flyerless\\FlyerlessClubManagement\\ModuleServiceProvider"
            ]
        }
    }
}
